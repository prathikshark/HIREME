<h1>Home</h1>
<% if current_user %>
    <p>Hello <%= current_user.email %></p>
    <p>Welcome</p>
    <p>You are a [<%= current_user.role %>]</p>
    <%= button_to 'Log Out', destroy_user_session_path, method: :delete %>
<% else %>
  <p>Please Sign In</p>
  <%= link_to 'Sign In', new_user_registration_path %>
  <%= link_to 'Log In', new_user_session_path %>
<% end %>
